[Unit]
Description=Collectiverse - xtuff.ai
After=network.target

[Service]
Type=exec
User=www-data
Group=www-data
WorkingDirectory=/home/wfz/xtuff/collectiverse
Environment=PATH=/usr/local/bin:/usr/bin:/bin
Environment=PYTHONPATH=/home/wfz/xtuff/collectiverse
ExecStart=/usr/bin/python3 -m streamlit run  app.py --server.port=8503
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target